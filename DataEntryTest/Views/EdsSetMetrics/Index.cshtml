@model IEnumerable<DataEntryTest.Models.EdsSetMetrics>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TargetCustomMetric)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActualCustomMetric)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkMetricsType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkSet)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.TargetCustomMetric)
            </th>
            <td>
                @Html.DisplayFor(modelItem => item.ActualCustomMetric)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkMetricsType.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkSet.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
